{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "parser": "@babel/eslint-parser",
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "class-methods-use-this": ["error", { "exceptMethods": ["validate"] }],
      "no-constructor-return": "off",
      "no-unused-vars": "off",
      "no-useless-constructor": "off",
      "consistent-return": "off"
    }
  }